<div class="row">
  <ul class="admin-list">
    Administrators
    <% @admins.each do |admin| %>
      <li><%= admin.username%></li>
      <li><%= admin.email%></li>
    <% end %>
  </ul>

  <ul class="member-list">
    Members
    <% @members.each do |member| %>
      <li><%= member.username%></li>
      <li><%= member.email%></li>
      <li><%= link_to "Delete This User", users_path, method: :delete %></li>
      <li><%= link_to "Make Admin", user_path(member), method: :put %></li>
      <br>
    <% end %>
  </ul>
</div>
